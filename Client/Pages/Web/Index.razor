@page "/"@using static MyVideoResume.Client.Shared.Resume.ResumePreviewComponent@inherits BaseComponent<PageTitle>MyVideoResu.ME - Promote your resume</PageTitle><SearchComponent></SearchComponent><RadzenRow Gap="5" RowGap="5">    <RadzenColumn Size="12">    </RadzenColumn></RadzenRow><RadzenRow Gap="5" RowGap="5" Style=" margin-bottom: 25px;">    <RadzenColumn Size="12">        <RadzenCard>            <RadzenStack AlignItems="AlignItems.Center" Orientation="Orientation.Horizontal" Style="overflow-x: scroll; max-width: 100%">                @foreach (var resume in ResumeList)                {                    <RadzenColumn Size="12" SizeXS="12" SizeSM="12" SizeMD="5" SizeLG="5" SizeXL="5" SizeXX="5">                        <ResumePreviewComponent ResumeItem=@resume Mode="ResumePreviewMode.View"></ResumePreviewComponent>                    </RadzenColumn>                }            </RadzenStack>        </RadzenCard>    </RadzenColumn></RadzenRow><JobResumeMatchTool></JobResumeMatchTool><SentimentAnalysisTool></SentimentAnalysisTool>