@page "/dashboard"@inherits BaseComponent<PageTitle>MyVideoResu.ME - Dashboard</PageTitle><RadzenRow Gap="5" RowGap="5">    <RadzenColumn Size="12">        <RadzenText Text="Dashboard" TextStyle="Radzen.Blazor.TextStyle.DisplayH2" TagName="Radzen.Blazor.TagName.Auto"></RadzenText>        <RadzenCard Style="margin-top: 40px; padding: 20px 40px 40px;">            <RadzenRow>                <RadzenColumn>                    <RadzenRow JustifyContent="JustifyContent.End" AlignItems="AlignItems.End" Gap="0">                        <RadzenButton ButtonStyle="ButtonStyle.Danger" Text="Create Resume" Click="@(args => NavigateTo("login"))"></RadzenButton>                    </RadzenRow>                    <RadzenRow JustifyContent="JustifyContent.Start" AlignItems="AlignItems.Start" Gap="0">                        <RadzenColumn>                            <RadzenCard class="rz-mx-auto" Style="max-width: 220px" Variant="Variant.Outlined">                                <RadzenStack Orientation="Orientation.Vertical" JustifyContent="JustifyContent.Start" Gap="1rem" class="rz-p-4">                                    <RadzenButton ButtonStyle="ButtonStyle.Danger" Text="Upload PDF" Click="@(args => NavigateTo("login"))"></RadzenButton>                                    <RadzenButton ButtonStyle="ButtonStyle.Danger" Text="Upload JSON" Click="@(args => NavigateTo("login"))"></RadzenButton>                                    <RadzenButton ButtonStyle="ButtonStyle.Danger" Text="Use Editor" Click="@(args => NavigateTo("login"))"></RadzenButton>                                    <RadzenCard Variant="Variant.Outlined">                                        <RadzenUpload id="ddUpload" @ref="uploadDD" ChooseText="Drag and drop here or click to choose files" Auto="false" Multiple="true" Url="upload/multiple" InputAttributes="@(new Dictionary<string,object>(){ { "aria-label", "select file" }})" Progress=@(args => OnProgress(args, "Drag and drop files to upload")) Style="width: 100%" />                                        <RadzenButton ButtonStyle="ButtonStyle.Danger"  Text="Upload File" Click=@(args => uploadDD.Upload()) class="rz-mt-4" />                                    </RadzenCard>                                </RadzenStack>                            </RadzenCard>                        </RadzenColumn>                        <RadzenColumn>                        </RadzenColumn>                    </RadzenRow>                    <!-- For Each REsume Loop through and display -->                </RadzenColumn>            </RadzenRow>        </RadzenCard>    </RadzenColumn></RadzenRow>